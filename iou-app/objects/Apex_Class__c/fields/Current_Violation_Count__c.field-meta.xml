<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Current_Violation_Count__c</fullName>
    <description>Calculated by summing all unresolved violations.</description>
    <externalId>false</externalId>
    <formula>Current_Bad_Practices__c +
 Current_Code_Smells__c +
 Current_Cost_to_Change_Test__c +
 Current_Cost_to_Understand_Maintain__c +
 Current_Ineffectual_Tests__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Current Violation Count</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
