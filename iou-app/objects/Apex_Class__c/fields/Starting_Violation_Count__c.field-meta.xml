<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Starting_Violation_Count__c</fullName>
    <description>Calculated by summing all benchmark starting violations.</description>
    <externalId>false</externalId>
    <formula>Starting_Cost_to_Understand_Maintain__c  + 
Starting_Cost_to_Change_Test__c + 
Starting_Code_Smells__c + 
Starting_Bad_Practices__c + 
Starting_Ineffectual_Tests__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Starting Violation Count</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
